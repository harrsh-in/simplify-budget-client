{
    "singleQuote": false,
    "semi": true,
    "arrowParens": "always",
    "endOfLine": "auto",
    "bracketSameLine": false,
    "jsxSingleQuote": false,
    "printWidth": 80,
    "quoteProps": "as-needed",
    "singleAttributePerLine": true,
    "tabWidth": 4,
    "useTabs": false,
    "trailingComma": "es5",
    "tailwindConfig": "./tailwind.config.js",
    "plugins": ["prettier-plugin-tailwindcss"]
}
